{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "title": "ManifestFiles",
  "properties": {
    "boo": {
      "type": "array",
      "items": {
        "anyOf": [
          {
            "type": "object",
            "properties": {
              "title": {
                "type": "string"
              },
              "filename": {
                "type": "string",
                "pattern": "\\.(jpg|jpeg|gif|png)$"
              },
              "landmark": {
                "type": "string"
              },
              "toc": {
                "type": "boolean"
              }
            },
            "required": ["title", "filename"]
          }
        ]
      }
    }
  }
}
